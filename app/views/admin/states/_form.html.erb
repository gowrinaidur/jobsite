<%= form_for([:admin,@state]) do |f| %>  
<div class="new_admin_form new_admin_states_form">
  <div class="field">
    <%= f.label :country_id %>
    <%= f.select :country_id,Country.all.map{|x| [x.name,x.id]},prompt: 'Select Country' %>
  </div>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="actions">
    <%= f.submit 'Save',class: 'btn btn-primary' %>
  </div>
</div>
<% end %>
